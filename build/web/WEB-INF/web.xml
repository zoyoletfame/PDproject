<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DialysisServlet</servlet-name>
        <servlet-class>Controller.DialysisServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoundServ</servlet-name>
        <servlet-class>Controller.RoundServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DBroundServ</servlet-name>
        <servlet-class>Controller.DBroundServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowBackServlet</servlet-name>
        <servlet-class>Model.ShowBackServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowDialysisEndServlet</servlet-name>
        <servlet-class>Controller.ShowDialysisEndServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recordTableServlet</servlet-name>
        <servlet-class>Controller.recordTableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resolveRecaordServlet</servlet-name>
        <servlet-class>Controller.resolveRecaordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteServlet</servlet-name>
        <servlet-class>Controller.deleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DialysisServlet</servlet-name>
        <url-pattern>/DialysisServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoundServ</servlet-name>
        <url-pattern>/RoundServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DBroundServ</servlet-name>
        <url-pattern>/DBroundServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowBackServlet</servlet-name>
        <url-pattern>/ShowBackServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowDialysisEndServlet</servlet-name>
        <url-pattern>/ShowDialysisEndServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>recordTableServlet</servlet-name>
        <url-pattern>/recordTableServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resolveRecaordServlet</servlet-name>
        <url-pattern>/resolveRecaordServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteServlet</servlet-name>
        <url-pattern>/deleteServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
